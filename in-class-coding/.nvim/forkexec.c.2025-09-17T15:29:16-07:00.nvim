#include <stdio.h>
#include <sys/types.h>
#include <unistd.h>

int main() {
  pid_t forkpid = fork();
  if (forkpid == 0) {
    char *args[] = {"/bin/echo", "ls -a -l -h", NULL};
    execv("/bin/echo", args);
  } else {
    char *args[] = {"/bin/echo", "ls -a", NULL};
    execv("/bin/echo", args);
  }
}
