#include <stdio.h>
#include <unistd.h>
#include <sys/types.h>

int main(){
  pid_t forkpid = fork();
  if (forkpid == 0){
    char* args[] = {"/bin/echo", "ls -a -l -h", NULL};
    execv("/bin/echo", args);
  } else {
    char* args[] = {"/bin/echo", "ls -a", NULL};
    execv("/bin/echo", args);
  }
}
