#include <stdio.h>
#include <unistd.h>
#include <sys/wait.h>
#include <stdlib.h>

int main (void) {
  int pid = fork();
  if (pid) {
    int waitstatus = 0;
    if (waitpid(pid, &waitstatus, 0) == -1) {
      perror("waitpid() failed");
      exit(EXIT_FAILURE);
    } else {
      printf("Parent done\n");
    }
  } else {
    printf("Child process\n");
  }
}
